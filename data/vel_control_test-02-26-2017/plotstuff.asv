load('step.mat')
xdes = linspace(0,8);
ydes = ones(numel(xdes))*0.5;
%simulation
figure(1)
plot(ydes, xdes,' xsim, ysim)
xlabel('Time (s)')
ylabel('Forward Velocity (m/s)')
title('Closed Loop: 0.5 m/s')
legend('Desired','Simulation', 'location', 'southeast')
xlim([0 8])

